{
  "name": "my-team-page-master",
  "version": "1.0.0",
  "description": "Team page",
  "default": "index.html",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "parcel build index.html --dist-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfredoGuacaran/my-team-page.git"
  },
  "author": "Alfredo Guacaran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlfredoGuacaran/my-team-page/issues"
  },
  "homepage": "https://github.com/AlfredoGuacaran/my-team-page#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.5",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.0",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "parcel": "^2.0.0-rc.0"
  }
}
